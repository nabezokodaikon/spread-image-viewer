{
  "name": "spread-image-viewer",
  "version": "0.1.0",
  "main": "dest/main.js",
  "description": "Spread image viewer",
  "dependencies": {
    "immutable": "^3.8.1",
    "log4js": "^1.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "electron": "^1.6.8",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.5.11",
    "jest": "^20.0.3",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf dest && rimraf .yarnclean && rimraf node_modules && yarn",
